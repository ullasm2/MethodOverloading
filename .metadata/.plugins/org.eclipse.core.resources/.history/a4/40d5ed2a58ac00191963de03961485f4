package org.india;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.ie.InternetExplorerDriver;

public class LaunchingBrowser {

	// System.setProperty("key","Value")
	public static void main(String[] args) {
		System.setProperty("webdriver.ie.driver","C:\\Users\\ullas\\eclipse-workspace\\SeleniumLaunchingBrowser\\Driver\\IEDriverServer.exe");
		
WebDriver m=new InternetExplorerDriver();
// To get the URL
m.get("https://www.facebook.com/");

// To get the Title

String title = m.getTitle();
System.out.println(title);

// To get the current URL

String currentUrl = m.getCurrentUrl();
System.out.println(currentUrl);








		
	}

}
